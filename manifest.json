{
  "manifest_version": 2,
  "name": "Chrome Scrum Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://meet.google.com/**-**-**"],
      "js": ["inject.js"]
    } 
  ],
  "web_accessible_resources": ["app.js", "app.css"],
  "externally_connectable": {
    "matches": ["*://meet.google.com/**-**-**"]
  }
}
